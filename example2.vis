<div id="body" class_str="body">
    <div id="menubar" class_str="menubar">
        <text class_str="menubar_text" text="discoword uwu :3"></>
    </>
    <div id="panels_container" class_str="panels_container">
        <div id="left_pane" class_str="left_pane">
            <div id="server_list" class_str="server_list">
                <text class_str="server_text" text="Servers"></>
            </>
            <div id="channel_list" class_str="channel_list">
                <text class_str="channel_text" text="Channels"></>
            </>
            <div id="user_info" class_str="user_info">
                <text class_str="username_text" text="[*] crystaltine"></>
                <text class_str="user_status" text="# online, visible"></>
            </>
        </>
        <div id="right_pane" class_str="right_pane">
            <div id="messages_pane" class_str="messages_pane"></>
            <div id="send_container" class_str="send_container">
                <input class_str="send_container_text" placeholder="Type a message..."></>
            </>
        </>
    </>
</>